{
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": 0,
    "max-len": [
      2,
      {
        "code": 120
      }
    ],
    "class-methods-use-this": [0]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "globals": {
    "protractor": true,
    "element": true,
    "browser": true,
    "by": true
  }
}
